{
    "name": "pegen-peg",
    "displayName": "Pegen PEG Grammar Highlighting",
    "description": "Syntax highlighting for the PEG syntax that the pegen project uses",
    "version": "0.1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/lysnikolaou/pegen-peg-highlighting"
    },
    "license": "MIT",
    "publisher": "lysnikolaou",
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "pegen",
            "aliases": ["Pegen PEG", "pegen"],
            "extensions": [".gram"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "pegen",
            "scopeName": "source.gram",
            "path": "./syntaxes/pegen.tmLanguage.json"
        }]
    }
}